{
    "name": "petrepatrasc/blizzard-starcraft-api",
    "type": "symfony-bundle",
    "description": "A PHP 5.3+ wrapper for the Starcraft 2 API",
    "keywords": ["PHP", "Starcraft 2", "API", "Blizzard", "Symfony"],
    "homepage": "http://petrepatrasc.github.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Petre Pătrașc",
            "email": "petre@dreamlabs.ro",
            "homepage": "http://www.petrepatrasc.ro"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "ext-curl": "*",
        "symfony/framework-bundle": ">=2.1",
        "symfony/security-bundle": ">=2.1"
    },
    "require-dev": {
        "twig/twig": "*",
        "doctrine/doctrine-bundle": "*",
        "symfony/yaml": "2.1.*",
        "symfony/validator": "2.1.*"
    },
    "autoload": {
        "psr-0": {
            "petrepatrasc\\BlizzardApiBundle": ""
        }
    },
    "target-dir": "petrepatrasc/BlizzardApiBundle"
}
